<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>
    <%= title %>
  </title>
</head>

<body>
  <form action="/users/search" method="post">
    id<input type="text" name="id" value="<%= datas.id %>"><br>
    账号<input type="text" name="account" value="<%= datas.account %>"><br>
    密码<input type="text" name="password" value="<%= datas.password %>"><br>
    最高分<input type="text" name="highestscore" value="<%= datas.highestscore %>"><br>
    <input type="submit" value="查询">
  </form>
  <table>
    <tr>
      <th>id</th>
      <th>账号</th>
      <th>密码</th>
      <th>最高分</th>
    </tr>
    <% for(var i=0; i < datas.length; i++) {%>
      <tr>
        <td>
          <%= datas[i].id %>
        </td>
        <td>
          <%= datas[i].account %>
        </td>
        <td>
          <%= datas[i].password %>            
        </td>
        <td>
          <%= datas[i].highestscore %>
        </td>
        <td><a href="/users/add">添加用户</a></td>
        <td><a href="/users/del/<%= datas[i].id %>">删除</a></td>
        <td><a href="/users/toUpdate/<%= datas[i].id %>">修改</a></td>
      </tr>
      <% } %>
  </table>
</body>

</html>
